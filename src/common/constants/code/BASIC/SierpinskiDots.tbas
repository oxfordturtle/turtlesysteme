REM SierpinskiDots

DIM x%(2)
DIM y%(2)
x%(0) = 400
y%(0) = 138
x%(1) = 843
y%(1) = 650
x%(2) = 179
y%(2) = 778
thisx% = RND(200) + 399
thisy% = RND(200) + 399
REPEAT
  thisc% = RND(3) - 1
  thisx% = (thisx% + x%(thisc%)) / 2
  thisy% = (thisy% + y%(thisc%)) / 2
  PIXSET(thisx%, thisy%, PURPLE)
UNTIL 0 = 1
END
